programa {
  funcao inicio() {
/**
 * Seu nome
 * Este exemplo utiliza a entrada de dados do Portugol para obter
 * e armazenar o nome do usuário em um variável. Logo após,
 * ultiliza a saída de dados para exibir o nome digitado.
 */
        cadeia nome

        escreva("Digite seu nome: ")
        leia(nome)

        escreva("Seu nome é " , nome)
        limpa
  }
}
